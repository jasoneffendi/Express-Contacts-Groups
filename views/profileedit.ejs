<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title></title>
    </head>

    <body>
        <thead>
            <tr>
                <th>ID</th>
                <th>Username</th>
                <th>Password</th>
                <th>Contact ID</th>
            </tr>
        </thead>
        <form class="" action="/profile/edit/<%=data[0].id%>" method="POST">
            <input type="text" name="username" placeholder="Username" value="<%=data[0].username%>">
            <input type="text" name="password" placeholder="Password" value="<%=data[0].password%>">
            <select name="idContacts">
                <% if(dataContact) { dataContact.forEach( contacts => { %>    
                    <% let selected = '' 
                      if (data.idContacts === contacts.id) { 
                        selected = 'selected'
                     } %>
                        <option value='<%=data.idContacts%>' <%= selected%> ><%=contacts.name%></option> 
                    <% })} %>      
            </select>
            <input type="submit" value="SUBMIT">
        </form>
    </body>
</html>