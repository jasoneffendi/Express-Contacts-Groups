<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title></title>
    </head>

    <body>
        <thead>
            <tr>
                <th>Name</th>
                <th>Company</th>
                <th>Telephone</th>
                <th>Email</th>
            </tr>
        </thead>
        <form class="" action="/contacts/edit/<%=data[0].id%>" method="POST">
            <input type="text" name="name" placeholder="Name" value="<%=data[0].name%>">
            <input type="text" name="company" placeholder="Company" value="<%=data[0].company%>">
            <input type="text" name="telp" placeholder="Telephone" value="<%=data[0].telp%>">
            <input type="text" name="email" placeholder="Email" value="<%=data[0].email%>">
            <input type="submit" value="SUBMIT">
        </form>
    </body>
</html>